.landing_headers{
  width:50%;

}

.landing_headers p{
  font-size:large;
      text-align: justify;
}

.hidden_background{
  display:none;
}

#highlights_cards .card{
  min-height: 20rem;
    min-width: 20rem;
}

#highlights_cards .card-body{
  background: #ffffffba!important;
}

#highlights_cards a{
  text-decoration: none;
}

#highlights_cards .btn-info{
  width: 100%;
  border-radius: 0;
  opacity:0.75;
}

#highlights_cards .btn-info a{
  color: white;
font-weight: lighter;
font-variant: small-caps;
font-size: large;
}

.icons-left-col{
  text-align: center;
  padding-left:25%;
}

.icons-right-col{
  text-align: center;
  padding-right:25%;
}

.text-right-col{
  border-left: 1px solid #efefef;
      /*margin-bottom: 10px;*/
      padding-left: 1px;
      padding-top: 10px;
}

.text-right-col .border-info{
  box-shadow: 1px 5px 1px 1px #0000001a;
}

.text-left-col{
  border-right: 1px solid #efefef;
      margin-bottom: 10px;
      padding-right: 1px;
      padding-top: 10px;
}

.text-left-col .border-info{
  box-shadow: -1px 5px 1px 1px #0000001a;
}


.icons-rows{
  padding-bottom:20px;
}

.title-element-fullwidth{
      background-color: #038f8b;
      padding-top:50px;
