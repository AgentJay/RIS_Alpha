  nav.fixed-top{
    z-index:1;
    margin: 20px 50px;
    padding: 0;
    height:60px;
  }

  nav.fixed-top .container{
    margin-left: 0%;
}

  nav.fixed-top .navbar-brand{
    padding:0px;
    margin:0px;
  }

  nav.fixed-top .navbar-brand img{
    height:96%;
    padding-top:2%;
    padding-bottom:2%;
  }

  .a_nostyle, .a_nostyle:hover{
    text-decoration:none;
    color:default;
  }

  .btn-xl {
  	text-transform: uppercase;
  	padding: 1.5rem 3rem;
  	font-size: .9rem;
  	font-weight: 700;
  	letter-spacing: .1rem;
  }
  .bg-black {
  	background-color: #000!important;
  }
.bg-dark{
  /*background-color:rgba(52, 58, 64, 0.73)!important;*/
  background-color: white!important;
}
.bg-light{
  background-color: rgba(255,255,255, 1)!important;
}


  .rounded-pill {
  	border-radius: 5rem;
  }
  .navbar-custom {
  	padding-top: 1rem;
  	padding-bottom: 1rem;
  	background-color: rgba(0, 0, 0, .7);
  }
  .navbar-custom .navbar-brand {
  	text-transform: uppercase;
  	font-size: 1rem;
  	letter-spacing: .1rem;
  	font-weight: 700;
  }
  .navbar-custom .navbar-nav .nav-item .nav-link {
  	text-transform: uppercase;
